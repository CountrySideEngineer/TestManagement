@page
@model TestManagement.Pages.TestCases.IndexModel

<h1>Test Cases</h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Description</th>
            <th>CreatedAt</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.TestCases)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Description</td>
                <td>@item.CreatedAt</td>
            </tr>
        }
    </tbody>
</table>
