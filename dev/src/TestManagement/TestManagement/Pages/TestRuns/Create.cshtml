@page
@model TestManagement.Pages.TestRuns.CreateModel

<validation-summary model-only></validation-summary>

<h2>新しいテスト実行</h2>

<form method="post">
    <input type="hidden" asp-for="TestRun.TestCaseId" />

    <div class="form-group">
        <label asp-for="TestRun.ExecutedAt"></label>
        <input asp-for="TestRun.ExecutedAt" class="form-control" />
    </div>

    <div class="form-group">
        <label asp-for="TestRun.Status"></label>
        <input asp-for="TestRun.Status" class="form-control" />
    </div>

    <button type="submit" class="btn btn-primary">登録</button>
</form>
